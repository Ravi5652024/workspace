package com.api.avatarbackend.entities;

import org.springframework.stereotype.Component;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Component
@Data

@NoArgsConstructor
public class DiamondAcquire {
	
	private int count;
	private  int validity_time;
	
	public DiamondAcquire(int count, int time) {
        this.count = count;
        this.validity_time = time;
    }

}
