import com.example.demo.request.DiamondUsageRequest;

public interface DiamondServiceInterface {

	public DiamondDetails addDiamond(DiamondDetails diamondDetails);

	public List<DiamondDetails> getAllDiamondPacks();

	public DiamondPurchasedHistory saveDiamondPurchased(DiamondPurchasedHistory diamondPurchasedHistory);
    
	
	private DiamondUsageRate saveDiamond(DiamondUsageRequest diamondUsageRequest);

	private DiamondUsageRate getDiamondById(String id);

	private List<DiamondUsageRate> getAllDiamonds();

    private void deleteDiamond(String id);

}
